{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/auth/register",
      "dest": "/api/auth/register.js"
    },
    {
      "src": "/api/auth/login",
      "dest": "/api/auth/login.js"
    },
    {
      "src": "/api/auth/logout",
      "dest": "/api/auth/logout.js"
    },
    {
      "src": "/api/transactions/stats",
      "dest": "/api/transactions/stats.js"
    },
    {
      "src": "/api/transactions/([^/]+)",
      "dest": "/api/transactions/[id].js"
    },
    {
      "src": "/api/transactions",
      "dest": "/api/transactions/index.js"
    },
    {
      "src": "/api/notifications/preferences",
      "dest": "/api/notifications/preferences.js"
    },
    {
      "src": "/api/notifications/taxes/([^/]+)",
      "dest": "/api/notifications/taxes/[id].js"
    },
    {
      "src": "/api/notifications/taxes",
      "dest": "/api/notifications/taxes.js"
    },
    {
      "src": "/api/notifications",
      "dest": "/api/notifications/index.js"
    },
    {
      "src": "/api/tips/generate",
      "dest": "/api/tips/index.js"
    },
    {
      "src": "/api/tips",
      "dest": "/api/tips/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}