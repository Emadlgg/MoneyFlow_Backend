{
  "version": 2,
  "builds": [
    {
      "src": "api/health.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/tips/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/auth/login.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/auth/register.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/tips/generate",
      "dest": "/api/tips/index.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/tips",
      "dest": "/api/tips/index.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/auth/login",
      "dest": "/api/auth/login.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/auth/register",
      "dest": "/api/auth/register.js",
      "methods": ["POST", "OPTIONS"]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}